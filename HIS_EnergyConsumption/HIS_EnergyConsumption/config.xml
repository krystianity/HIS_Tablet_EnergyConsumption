<?xml version='1.0' encoding='utf-8'?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="io.cordova.HISEnergyConsumption" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>HIS_EnergyConsumption</name>
  <description>HIS app that gives information about the energy consumption of a managable list of eletronic devices.</description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">C. Froehlingsdorf, D. Stodko</author>
  <content src="index.html" />
  <access origin="*" />
  <vs:features />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.0" />
  <preference name="windows-phone-target-version" value="8.1" />
  <preference name="UIWebViewBounce" value="false" />
</widget>
